      {{!Populating cube partial with properties named as in cube object}}
      <div class="cube">
        <img class="cube" src="{{imageUrl}}">
        <p class="name">{{name}}</p>
        <p><span>Difficulty level:</span> {{difficulty}}</p>
        <a class="btn" href="/details/{{id}}">Details</a>
        <button class="btn">SEE MORE</button>
        <div class="cube-description">{{description}}</div>
      </div>